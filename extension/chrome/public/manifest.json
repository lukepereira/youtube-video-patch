{
    "short_name": "YouTube™ Video Regenerator",
    "name": "YouTube™ Video Regenerator",
    "description": "YouTube™ Video Regenerator is an extension for replacing deleted or unavailable videos.",
    "version": "0.0.3",
    "manifest_version": 2,
    "background": {
        "scripts": ["app/background.js"]
    },
    "browser_action": {},
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["/static/js/content.js"]
        }
    ],
    "web_accessible_resources": ["/static/css/content.css"],
    "permissions": [
        "storage",
        "tabs",
        "identity",
        "https://us-central1-youtube-tools-245705.cloudfunctions.net/*"
    ],
    "key": "",
    "oauth2": {
        "client_id": "992820074494-04qgln9cr9gm2r1heancpfmv3fsp3tf9.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/plus.login",
            "https://www.googleapis.com/auth/chromewebstore.readonly"
        ]
    },
    "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}
